<div>
  <h2>Lista de productos</h2>

  {{#each readProduct}}
    <p>
      ID:
      {{this.id}}
      Name:
      {{this.title}}
      Img:
      {{this.img}}
      Description:
      {{this.description}}
      Stock:
      {{this.stock}}
      Code:
      {{this.code}}
    </p>
    <hr />

  {{/each}}
</div>